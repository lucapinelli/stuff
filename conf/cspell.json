// cSpell settings used by the `sh/spell` script
{
  // version - version of the setting file. Always 0.2
  "version": "0.2",
  // language - current active spelling language
  "language": "en",
  // words - list of words to be always considered correct
  "words": [
    // people names and surnames
    "abdou",
    "aziz",
    "badenhorst",
    "bugeja",
    "cachinero",
    "camilleri",
    "curro",
    "darko",
    "hoosen",
    "luca",
    "marita",
    "mosner",
    "pensenti",
    "pinelli",
    "raboy",
    "scaleia",
    "suryanda",
    // brands / products
    "affectli",
    "flowable",
    "keycloak",
    "rocketchat",
    "salvimar",
    "suunto",
    // it
    "cpus",
    "cucumberopen",
    "decostructing",
    "denormalize",
    "distro",
    "geospatial",
    "graphiql",
    "hostpath",
    "ojson",
    "printenv",
    "typeahead",
    "unregister",
    "unstaged",
    // valid words missing in the dictionary
    "freedive",
    "matryoshka",
    "moka",
    ""
  ],
  // flagWords - list of words to be always considered incorrect
  // This is useful for offensive words and common spelling errors.
  "flagWords": [],
  // ignoreWords - a list of words to be ignored (even if they are in the flagWords)
  "ignoreWords": [],
  // ignorePaths- a list of globs to specify which files are to be ignored (e.g. ["node_modules/**"])
  "ignorePaths": []
}
